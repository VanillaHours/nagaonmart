<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mart.nagaon">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Firebasedata"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Address"></activity>
        <activity android:name=".cart" />
        <activity android:name=".product.productpg" />
        <activity android:name=".categories.catgPage" />
        <activity android:name=".home.homepage" />
        <activity android:name=".signpage" />
        <activity android:name=".loginpage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>